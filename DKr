<script>
    // ============ CLICK TRACKING SYSTEM ============
    
    // Initialize tracking data
    let trackingData = JSON.parse(localStorage.getItem('trackingData')) || {
        clicks: [],
        totalClicks: 0,
        sections: {},
        projects: {},
        lastUpdated: new Date().toISOString()
    };
    
    // Track click function
    function trackClick(category, label, url = null) {
        const timestamp = new Date().toISOString();
        const clickData = {
            category,
            label,
            url,
            timestamp,
            userAgent: navigator.userAgent,
            screenSize: `${window.innerWidth}x${window.innerHeight}`
        };
        
        // Add to tracking data
        trackingData.clicks.push(clickData);
        trackingData.totalClicks++;
        
        // Update section-specific tracking
        if (!trackingData.sections[category]) {
            trackingData.sections[category] = {};
        }
        if (!trackingData.sections[category][label]) {
            trackingData.sections[category][label] = 0;
        }
        trackingData.sections[category][label]++;
        
        // Save to localStorage
        trackingData.lastUpdated = timestamp;
        localStorage.setItem('trackingData', JSON.stringify(trackingData));
        
        // Send to Google Analytics if available
        if (typeof gtag !== 'undefined') {
            gtag('event', 'click', {
                'event_category': category,
                'event_label': label,
                'value': url
            });
        }
        
        // Update analytics display
        updateAnalytics();
        
        console.log('Click tracked:', clickData);
    }
    
    // ============ PROJECT MANAGEMENT SYSTEM ============
    
    // Initialize projects
    let projects = JSON.parse(localStorage.getItem('projects')) || [];
    
    // Open project modal
    function openProjectModal() {
        document.getElementById('projectModal').classList.add('active');
        trackClick('modal', 'open-add-project');
    }
    
    // Close project modal
    function closeProjectModal() {
        document.getElementById('projectModal').classList.remove('active');
        document.getElementById('projectForm').reset();
    }
    
    // Add project
    function addProject(event) {
        event.preventDefault();
        
        const project = {
            id: Date.now(),
            title: document.getElementById('projectTitle').value,
            description: document.getElementById('projectDescription').value,
            url: document.getElementById('projectUrl').value,
            status: document.getElementById('projectStatus').value,
            clicks: 0,
            views: 0,
            createdAt: new Date().toISOString()
        };
        
        projects.push(project);
        localStorage.setItem('projects', JSON.stringify(projects));
        
        renderProjects();
        closeProjectModal();
        updateAnalytics();
        
        trackClick('project', 'add', project.title);
    }
    
    // Delete project
    function deleteProject(id) {
        if (confirm('Are you sure you want to delete this project?')) {
            const project = projects.find(p => p.id === id);
            projects = projects.filter(p => p.id !== id);
            localStorage.setItem('projects', JSON.stringify(projects));
            renderProjects();
            updateAnalytics();
            trackClick('project', 'delete', project?.title);
        }
    }
    
    // Track project click
    function trackProjectClick(id, url) {
        const project = projects.find(p => p.id === id);
        if (project) {
            project.clicks++;
            localStorage.setItem('projects', JSON.stringify(projects));
            renderProjects();
            updateAnalytics();
            trackClick('project-link', project.title, url);
            window.open(url, '_blank');
        }
    }
    
    // View project details
    function viewProjectDetails(id) {
        const project = projects.find(p => p.id === id);
        if (project) {
            project.views++;
            localStorage.setItem('projects', JSON.stringify(projects));
            renderProjects();
            trackClick('project-view', project.title);
            alert(`Project: ${project.title}\n\nDescription: ${project.description}\n\nURL: ${project.url}\n\nClicks: ${project.clicks}\nViews: ${project.views}\nCreated: ${new Date(project.createdAt).toLocaleDateString()}`);
        }
    }
    
    // Render projects
    function renderProjects() {
        const grid = document.getElementById('projectsGrid');
        
        if (projects.length === 0) {
            grid.innerHTML = `
                <div style="grid-column: 1 / -1; text-align: center; padding: 60px 20px;">
                    <div style="font-size: 3rem; margin-bottom: 20px;">üìÇ</div>
                    <h3 style="color: var(--text-secondary); margin-bottom: 10px;">No projects yet</h3>
                    <p style="color: var(--text-secondary);">Click "Add Project" to get started</p>
                </div>
            `;
            return;
        }
        
        grid.innerHTML = projects.map(project => `
            <div class="project-card">
                <div class="project-header">
                    <div>
                        <div class="project-title">${project.title}</div>
                        <span class="project-status status-${project.status}">${project.status.toUpperCase()}</span>
                    </div>
                </div>
                <p class="project-description">${project.description}</p>
                <a href="#" class="project-url" onclick="trackProjectClick(${project.id}, '${project.url}'); return false;">${project.url}</a>
                <div class="project-stats">
                    <div class="project-stat">
                        <div class="project-stat-label">Clicks</div>
                        <div class="project-stat-value">${project.clicks}</div>
                    </div>
                    <div class="project-stat">
                        <div class="project-stat-label">Views</div>
                        <div class="project-stat-value">${project.views}</div>
                    </div>
                </div>
                <div class="project-actions">
                    <button class="project-action-btn" onclick="viewProjectDetails(${project.id})">View Details</button>
                    <button class="project-action-btn delete-btn" onclick="deleteProject(${project.id})">Delete</button>
                </div>
            </div>
        `).join('');
    }
    
    // ============ ANALYTICS DASHBOARD ============
    
    function updateAnalytics() {
        // Total clicks
        document.getElementById('totalClicks').textContent = trackingData.totalClicks;
        
        // Project views
        const totalProjectViews = projects.reduce((sum, p) => sum + p.views, 0);
        document.getElementById('projectViews').textContent = totalProjectViews;
        
        // Active projects
        const activeProjectsCount = projects.filter(p => p.status === 'active').length;
        document.getElementById('activeProjects').textContent = activeProjectsCount;
        
        // Engagement rate (simplified calculation)
        const engagementRate = projects.length > 0 
            ? Math.min(100, Math.round((totalProjectViews / projects.length) * 10))
            : 0;
        document.getElementById('engagementRate').textContent = engagementRate + '%';
    }
    
    // ============ MOBILE MENU ============
    
    function toggleMobileMenu() {
        const menu = document.getElementById('navMenu');
        const toggle = document.querySelector('.menu-toggle');
        menu.classList.toggle('active');
        toggle.classList.toggle('active');
    }
    
    // Close mobile menu when clicking a link
    document.querySelectorAll('nav a').forEach(link => {
        link.addEventListener('click', () => {
            const menu = document.getElementById('navMenu');
            const toggle = document.querySelector('.menu-toggle');
            menu.classList.remove('active');
            toggle.classList.remove('active');
        });
    });
    
    // ============ EXPORT TRACKING DATA ============
    
    function exportTrackingData() {
        const dataStr = JSON.stringify(trackingData, null, 2);
        const dataBlob = new Blob([dataStr], { type: 'application/json' });
        const url = URL.createObjectURL(dataBlob);
        const link = document.createElement('a');
        link.href = url;
        link.download = `tracking-data-${new Date().toISOString().split('T')[0]}.json`;
        link.click();
        trackClick('analytics', 'export-data');
    }
    
    // Add export button to console
    console.log('%cüìä Analytics System Active', 'color: #00E5FF; font-size: 16px; font-weight: bold;');
    console.log('%cTo export tracking data, run: exportTrackingData()', 'color: #00FFA3;');
    console.log('%cCurrent tracking data:', 'color: #8BA3B0;', trackingData);
    
    // ============ INITIALIZATION ============
    
    // Initialize on page load
    window.addEventListener('DOMContentLoaded', () => {
        renderProjects();
        updateAnalytics();
        
        // Track page view
        trackClick('page', 'view', window.location.href);
    });
    
    // Track scroll depth
    let maxScroll = 0;
    window.addEventListener('scroll', () => {
        const scrollPercent = Math.round((window.scrollY / (document.body.scrollHeight - window.innerHeight)) * 100);
        if (scrollPercent > maxScroll && scrollPercent % 25 === 0) {
            maxScroll = scrollPercent;
            trackClick('scroll', `depth-${scrollPercent}%`);
        }
    });
    
    // Track time on page
    let timeOnPage = 0;
    setInterval(() => {
        timeOnPage += 30;
        if (timeOnPage % 60 === 0) {
            trackClick('engagement', `time-${timeOnPage}s`);
        }
    }, 30000); // Every 30 seconds
</script>
```

</body>
</html>
